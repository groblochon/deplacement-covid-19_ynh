{
    "name": "deplacement-covid-19",
    "id": "deplacement-covid-19",
    "packaging_format": 1,
    "description": {
        "en": "Générateur de certificat de déplacement.",
        "fr": "Générateur de certificat de déplacement."
    },
    "version": "0.0.1~ynh1",
    "url": "https://github.com/lab-mi/deplacement-codiv-19",
    "license": "MIT",
    "maintainer": {
        "name": ""
    },
    "requirements": {
        "yunohost": ">= 3.5"
    },
    "multi_instance": true,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain",
                "ask": {
                    "en": "Choose a domain for deplacement-covid-19",
                    "fr": "Choisissez un nom de domaine pour deplacement-covid-19"
                },
                "example": "example.com"
            },
            {
                "name": "path",
                "type": "path",
                "ask": {
                    "en": "Choose a path for deplacement-covid-19",
                    "fr": "Choisissez un chemin pour deplacement-covid-19"
                },
                "example": "/deplacement-covid-19",
                "default": "/deplacement-covid-19"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "ask": {
                    "fr": "Est-ce une application publique ?"
                },
                "default": true
            }
        ]
    }
}
